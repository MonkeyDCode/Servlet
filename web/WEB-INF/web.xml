<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Hola</servlet-name>
        <servlet-class>mx.uaemex.fi.diseno.agosto.Hola</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tarea</servlet-name>
        <servlet-class>mx.uaemex.fi.diseno.agosto.Tarea</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Goya</servlet-name>
        <servlet-class>mx.uaemex.fi.diseno.agosto.Goya</servlet-class>
        <init-param>
            <param-name>porra1</param-name>
            <param-value>goya</param-value>
        </init-param>
        <init-param>
            <param-name>porra2</param-name>
            <param-value>cachun</param-value>
        </init-param>
        <init-param>
            <param-name>porra3</param-name>
            <param-value>rra</param-value>
        </init-param>
        <init-param>
            <param-name>repeticiones</param-name>
            <param-value>2</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>Random</servlet-name>
        <servlet-class>mx.uaemex.fi.diseno.agosto.Random2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Hola</servlet-name>
        <url-pattern>/Hola</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tarea</servlet-name>
        <url-pattern>/Tarea</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Goya</servlet-name>
        <url-pattern>/Goya</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Random</servlet-name>
        <url-pattern>/Random</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
